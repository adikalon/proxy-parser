{
  "name": "proxy-parser",
  "description": "Парсер прокс-серверов на Electron JS",
  "version": "1.0.0",
  "author": "adikalon",
  "license": "ISC",
  "scripts": {
    "clear": "node clear.js",
    "build": "node clear.js && tsc && vue-cli-service build",
    "tsc": "tsc",
    "start": "electron app.js",
    "watch": "NODE_ENV=DEV vue-cli-service serve & sleep 5 && NODE_ENV=DEV electron app.js",
    "postinstall": "electron-builder install-app-deps && tsc && vue-cli-service build"
  },
  "dependencies": {
    "cheerio": "1.0.0-rc.3",
    "electron": "15.5.5",
    "sqlite3": "4.1.0",
    "vue": "2.6.10",
    "vue-router": "3.1.3"
  },
  "devDependencies": {
    "@types/cheerio": "0.22.13",
    "@types/sqlite3": "3.1.5",
    "@types/node": "^12.7.5",
    "@types/rimraf": "2.0.2",
    "@vue/cli-service": "3.11.0",
    "electron-builder": "21.2.0",
    "rimraf": "3.0.0",
    "typescript": "3.6.3",
    "vue-template-compiler": "2.6.10"
  }
}
